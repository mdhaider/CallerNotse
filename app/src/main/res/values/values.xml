<resources>
    <color name="contact_badge_line_dark">#ff000000</color>
    <color name="contact_badge_line_light">#ffffffff</color>
    <color name="contact_badge_overlay_dark">#aa888888</color>
    <color name="contact_badge_overlay_light">#aa888888</color>
    <color name="contact_badge_triangle_dark">#cc888888</color>
    <color name="contact_badge_triangle_light">#cc1f1f1f</color>
    <color name="text_color_primary_dark">#FFFAFAFA</color>
    <color name="text_color_primary_light">#FF212121</color>
    <color name="text_color_secondary_dark">#FF9E9E9E</color>
    <color name="text_color_secondary_light">#FFBDBDBD</color>
    <declare-styleable name="ContactPicker"><attr format="color|reference" name="cp_textColorPrimary"/><attr format="color|reference" name="cp_textColorSecondary"/><attr format="reference" name="cp_badgeLineColor"/><attr format="reference" name="cp_badgeTriangleColor"/><attr format="reference" name="cp_badgeOverlayColor"/><attr format="reference" name="cp_actionIconOK"/><attr format="reference" name="cp_actionIconCancel"/><attr format="reference" name="cp_actionIconCheckAll"/><attr format="reference" name="cp_actionIconSearch"/></declare-styleable>
    <plurals name="cp_group_description">
        <item quantity="one">(%1$d contact)</item>
        <item quantity="other">(%1$d contacts)</item>
    </plurals>
    <string name="cp_actionmode_selected">%d selected</string>
    <string name="cp_contact_tab_title">Contacts</string>
    <string name="cp_group_tab_title">Groups</string>
    <string name="cp_limit_reached">You can\'t pick more than %d contacts!</string>
    <string name="cp_no_contacts">No Contacts</string>
    <string name="cp_no_groups">No Groups</string>
    <style name="ContactPicker_Theme_Dark" parent="@style/Theme.AppCompat">
        <item name="cp_textColorPrimary">@color/text_color_primary_dark</item>
        <item name="cp_textColorSecondary">@color/text_color_secondary_dark</item>

        <item name="cp_badgeLineColor">@color/contact_badge_line_dark</item>
        <item name="cp_badgeTriangleColor">@color/contact_badge_triangle_dark</item>
        <item name="cp_badgeOverlayColor">@color/contact_badge_overlay_dark</item>

        <item name="cp_actionIconOK">@drawable/ic_done_white_24dp</item>
        <item name="cp_actionIconCancel">@drawable/ic_close_white_24dp</item>
        <item name="cp_actionIconCheckAll">@drawable/ic_apps_white_24dp</item>
        <item name="cp_actionIconSearch">@drawable/ic_search_white_24dp</item>
    </style>
    <style name="ContactPicker_Theme_Light" parent="@style/Theme.AppCompat.Light">
        <item name="cp_textColorPrimary">@color/text_color_primary_light</item>
        <item name="cp_textColorSecondary">@color/text_color_secondary_light</item>

        <item name="cp_badgeLineColor">@color/contact_badge_line_light</item>
        <item name="cp_badgeTriangleColor">@color/contact_badge_triangle_light</item>
        <item name="cp_badgeOverlayColor">@color/contact_badge_overlay_light</item>

        <item name="cp_actionIconOK">@drawable/ic_done_black_24dp</item>
        <item name="cp_actionIconCancel">@drawable/ic_close_black_24dp</item>
        <item name="cp_actionIconCheckAll">@drawable/ic_apps_black_24dp</item>
        <item name="cp_actionIconSearch">@drawable/ic_search_black_24dp</item>
    </style>
</resources>